{
  "name": "nedii-api",
  "version": "2.0.0",
  "description": "Nedii API",
  "main": "manage.py",
  "scripts": {
    "start": "python manage.py runserver 0.0.0.0:8000",
    "static": "python manage.py collectstatic --noinput",
    "migrations": "python manage.py makemigrations",
    "migrate": "python manage.py migrate",
    "freeze": "python -m pip freeze -l > requirements.txt",
    "deploy": "node build-and-deploy.js",
    "deploy:prod": "node build-and-deploy.js --production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org:Yudhs/nedii-api.git"
  },
  "keywords": [
    "Nedii"
  ],
  "author": "Christopher Irak Guzman Monsalvo",
  "license": "Copyright",
  "homepage": "https://bitbucket.org/Yudhs/nedii-api#readme",
  "devDependencies": {
    "axios": "^0.26.0"
  }
}
